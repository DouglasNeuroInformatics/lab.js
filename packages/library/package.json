{
  "name": "lab.js",
  "version": "23.0.0-alpha2",
  "description": "Building blocks for web-based behavioral experiments",
  "homepage": "https://lab.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixhenninger/lab.js.git"
  },
  "license": "Apache-2.0",
  "author": "Felix Henninger <mailbox@felixhenninger.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/es6/index.js",
      "types": "./dist/es6/index.d.ts"
    },
    "./canvas": {
      "import": "./dist/es6/canvas/index.js",
      "types": "./dist/es6/canvas/index.d.ts"
    },
    "./core": {
      "import": "./dist/es6/core/index.js",
      "types": "./dist/es6/core/index.d.ts"
    },
    "./css": {
      "import": "./dist/css/lab.css",
      "require": "./dist/css/lab.css"
    },
    "./data": {
      "import": "./dist/es6/data/index.js",
      "types": "./dist/es6/data/index.d.ts"
    },
    "./flow": {
      "import": "./dist/es6/flow/index.js",
      "types": "./dist/es6/flow/index.d.ts"
    },
    "./html": {
      "import": "./dist/es6/html/index.js",
      "types": "./dist/es6/html/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/es6/plugins/index.js",
      "types": "./dist/es6/plugins/index.d.ts"
    },
    "./util": {
      "import": "./dist/es6/util/index.js",
      "types": "./dist/es6/util/index.d.ts"
    }
  },
  "main": "dist/es6/index.js",
  "types": "dist/es6/index.d.ts",
  "scripts": {
    "build": "yarn build:js && yarn build:css && yarn build:legacy && yarn build:starterkit",
    "build:css": "postcss --local-plugins -u postcss-preset-env -o dist/css/lab.css src/starterkit/lib/lab.css",
    "build:js": "tsc",
    "build:legacy": "node scripts/build-legacy.js",
    "build:starterkit": "node scripts/bundle-starterkit.js",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:js\"",
    "dev:css": "postcss --watch --local-plugins -u postcss-preset-env -o dist/css/lab.css src/starterkit/lib/lab.css",
    "dev:js": "tsc --watch",
    "lint": "eslint --fix src",
    "prepare": "yarn run build:starterkit",
    "prettier": "prettier --write src",
    "test": "karma start test/_karma/ci.conf.js",
    "test:base": "jest --detect-open-handles",
    "test:coverage": "yarn run build:js:coverage && karma start test/_karma/coverage.conf.js",
    "test:sauce": "karma start test/_karma/sauce.conf.js"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.0",
    "lodash": "^4.17.21",
    "seedrandom": "^3.0.5",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "4.14.108",
    "@types/seedrandom": "2.4.27",
    "@types/ua-parser-js": "^0.7.35",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-preset-env": "^7.7.2",
    "requirejs": "^2.3.3",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "sinon": "^14.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  }
}
